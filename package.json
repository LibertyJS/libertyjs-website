{
  "name": "libertyjs-2017",
  "version": "1.0.0",
  "description": "LibertyJS 2017 WordPress Template",
  "main": "index.js",
  "scripts": {
    "build": "npm run check-node-version && gulp",
    "check-node-version": "check-node-version --package",
    "hooks": "./bin/install-hooks.sh",
    "eslint": "eslint **/*.js",
    "lint": "npm run check-node-version && npm run stylelint && npm run eslint && npm run phplint",
    "phplint": "./vendor/bin/phpcs --standard=phpcs.xml $(git ls-files | grep .php$)",
    "start": "npm run check-node-version && npm install && composer update && npm run hooks && gulp watch",
    "stylelint": "stylelint css/*.css -q"
  },
  "engines": {
    "node": ">=8.9.3",
    "npm": ">=5.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:LibertyJS/libertyjs2017.git"
  },
  "author": "Jonathan Belcher",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/LibertyJS/libertyjs2017/issues"
  },
  "homepage": "https://github.com/LibertyJS/libertyjs2017",
  "devDependencies": {
    "check-node-version": "3.2.0",
    "cssnano": "3.10.0",
    "eslint": "4.17.0",
    "eslint-config-wpcalypso": "2.0.0",
    "eslint-plugin-wpcalypso": "4.0.1",
    "gulp": "3.9.1",
    "gulp-imagemin": "4.1.0",
    "gulp-postcss": "7.0.1",
    "gulp-sourcemaps": "2.6.4",
    "gulp-watch": "5.0.0",
    "postcss": "6.0.17",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.0.0",
    "stylelint": "8.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "font-awesome": "^4.7.0"
  }
}
